{
  "name": "alphaibanez/laminim-service",
  "description": "Laminim Kernel Tools Backend Service",
  "type": "library",
  "version": "0.0.1",
  "require": {
    "php": ">=8.1.0",
    "ext-curl": "*",
    "ext-imap": "*",
    "ext-intl": "*",
    "ext-openssl": "*",
    "nikic/fast-route": "^1.3",
    "cocur/slugify": "^4.2",
    "nesbot/carbon": "^2.62",
    "robmorgan/phinx": "^0.13.4",
    "symfony/console": "^6.2",
    "poliander/cron": "^3.0"
  },
  "require-dev": {
    "aws/aws-sdk-php": "^3.285",
    "influxdata/influxdb-client-php": "^3.2",
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lkt\\": "src"
    },
    "files": [
      "src/startup.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
